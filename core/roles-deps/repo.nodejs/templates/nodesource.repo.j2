enabled=1
gpgcheck=1
gpgkey=https://rpm.nodesource.com/pub/el/NODESOURCE-GPG-SIGNING-KEY-EL

[nodesource-source]
name=Node.js for Enterprise Linux {{ dist_major_version }} - $basearch - Source
baseurl=https://rpm.nodesource.com/pub_16.x/{{ version }}/{{ dist_major_version }}/SRPMS
